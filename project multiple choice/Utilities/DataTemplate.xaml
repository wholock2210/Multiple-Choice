<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:project_multiple_choice.ViewModels"
                    xmlns:v="clr-namespace:project_multiple_choice.Views">
    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <v:Home/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DashboardVM}">
        <v:Dashboard/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:InfoVM}">
        <v:Info/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <v:Setting/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TextVM}">
        <v:Text/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:EditorVM}">
        <v:Editor/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TestVM}">
        <v:Test/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ChooseExamVM}">
        <v:ChooseExam/>
    </DataTemplate>
</ResourceDictionary>